class MyHashSet {
public:
    vector<bool> vec;
    
    MyHashSet() {
        vec.resize(1e6 + 1, false);
    }
    
    void add(int key) {
        vec[key] = true;
    }
    
    void remove(int key) {
        vec[key] = false;
    }
    
    bool contains(int key) {
        return vec[key];
    }
};

