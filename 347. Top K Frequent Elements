class Solution {
public:
    
    bool static comparator(pair<int, int>& e1, pair<int, int>& e2){
        return e1.second > e2.second;
    }
    
    vector<int> topKFrequent(vector<int>& nums, int k) {
        unordered_map<int, int> mp;
        for(int i=0; i<nums.size(); i++){
            mp[nums[i]]++;
        }
        
        vector<pair<int, int>> vec(mp.begin(), mp.end());
        
        sort(vec.begin(), vec.end(), comparator);
        
        vector<int> ans;
        vector<pair<int, int>>:: iterator itr = vec.begin();
        
        while(itr != vec.end() and k!=0){
            ans.push_back(itr->first);
            itr++; k--;
        }
        return ans;
    }
};


// more optimised solution taking advantage of priority queue. piority queue nothing but the maxheap sorts pair based on the first value.

class Solution {
public:
    
    vector<int> topKFrequent(vector<int>& nums, int k) {
        unordered_map<int, int> mp;
        for(int val : nums){
            mp[val]++;
        }
        
        priority_queue<pair<int, int>> pq;
        
        unordered_map<int, int>:: iterator itr = mp.begin();
        while(itr != mp.end()){
            pq.push({itr->second, itr->first});
            itr++;
        }
        vector<int> ans;
        
        while(k != 0){
            ans.push_back(pq.top().second);
            pq.pop();
            k--;
        }
        
        return ans;
    }
};
